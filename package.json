{
  "name": "nuxt-elm",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "[easytuan] <[easytuan@foxmail.com]>",
  "private": true,
  "license": "GPL",
  "scripts": {
    "dev": "nuxt",
    "start": "nuxt build && pm2 start ./node_modules/nuxt/bin/nuxt -- start --node-args='--harmony'",
    "stop": "pm2 stop ./node_modules/nuxt/bin/nuxt -- start",
    "restart": "nuxt build && pm2 restart ./node_modules/nuxt/bin/nuxt -- start --node-args='--harmony'",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "tep": "node template",
    "precommit": "npm run lint"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": 3000
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "js-cookie": "^2.2.0",
    "js-md5": "^0.7.3",
    "mint-ui": "^2.2.13",
    "moment": "^2.22.2",
    "nuxt": "^1.0.0",
    "pm2": "^3.1.2",
    "postcss-loader": "^3.0.0"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/proxy": "^1.2.4",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "less-loader": "^4.0.5",
    "node-sass": "^4.5.3",
    "postcss-loader": "^3.0.0",
    "postcss-px2rem": "^0.3.0",
    "sass-loader": "^6.0.5",
    "sass-resources-loader": "^1.3.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
